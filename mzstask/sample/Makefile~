CC=gcc
AR=ar
LD=ld
AS=as

SRCS = $(wildcard *.c)

PROGS = $(patsubst %.c,%,$(SRCS))
CFLAGS = -g -lzs -lmzstask -L../ -L../../zsrmm/lib -I../ -I../../zsrmm/

#response:	simple_response_time_enforcement.c
#	$(CC) -o $@ $<  busy.c $(CFLAGS) 

all: $(PROGS)

%: %.c
	$(CC) -o $@ $< $(CFLAGS)

clean:
	rm *.o $(PROGS)

